{
    "name": "angeloo/laravel-me",
    "description": "",
    "keywords": [
        "angeloo",
        "laravel-me"
    ],
    "homepage": "https://github.com/angeloo/laravel-me",
    "license": "MIT",
    "authors": [
        {
            "name": "Angelo Maiorano",
            "email": "angelo.maiorano@gmail.com",
            "homepage": "https://angelo-maiorano.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "laravel/legacy-factories": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Angeloo\\Me\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Angeloo\\Me\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Angeloo\\Me\\MeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
